{
  "name": "flashplayer",
  "version": "1.1",
  "productName": "flashplayer",
  "executableName": "flashplayer",
  "description": "A flash player!",
  "license": "AGPL-3.0",
  "main": "./scripts/main.js",
  "author": {
    "name": "Allan Niles",
    "url": "https://github.com/allancoding"
  },
  "bin": {
    "flashplayer": "./scripts/main.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allancoding/flashplayer"
  },
  "build": {
    "appId": "dev.allancoding.flashplayer",
    "files": [
      "./flash/**/*",
      "./scripts/*",
      "./pages/*",
      "./demo/**/*",
      "./icons/*"
    ],
    "asarUnpack": [
      "./flash/**/*"
    ]
  },
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "postinstall": "npmpd"
  },
  "devDependencies": {
    "@electron-forge/cli": "latest",
    "@electron-forge/maker-deb": "latest",
    "@electron-forge/maker-dmg": "latest",
    "@electron-forge/maker-squirrel": "latest",
    "@electron-forge/maker-zip": "latest",
    "@rabbitholesyndrome/electron-forge-maker-portable": "latest",
    "@reforged/maker-appimage": "",
    "electron": "^11.5.0",
    "electron-reloader": "latest"
  },
  "dependencies": {
    "electron-is-dev": "latest",
    "electron-storage": "latest",
    "npm-platform-dependencies": "latest",
    "update-electron-app": "latest"
  },
  "win32Dependencies": {
    "electron-squirrel-startup": "latest"
  }
}
